# DO NOT EDIT BELOW THIS LINE
ls --color -d . &>/dev/null 2>&1 && alias ls="ls --color=tty" || alias ls="ls -G"
alias lsa='ls -lah'
alias la='ls -a'
alias ll='ls -l'
alias lh='ls -a | egrep "^\."'

alias rm='rm -i'

alias vi="vim --noplugin"

alias cdd="cd $HOME/Dropbox/Dotfiles"
alias cds="cd $HOME/Dropbox/Scripts"
