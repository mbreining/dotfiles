# DO NOT EDIT BELOW THIS LINE
[push]
  default = tracking
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
[format]
  #pretty = %Cblue%h%Creset %Cgreen[%cr]%Creset (%an) %s
[alias]
  a = add
  co = checkout
  ci = commit
  ca = commit --amend
  di = diff
  dic = diff --cached
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  l = log --pretty=\"%Cblue%h%Creset %Cgreen[%cr]%Creset %s\"
  pl = pull
  ps = push
  plre = pull --rebase
  r = reflog
  st = status
